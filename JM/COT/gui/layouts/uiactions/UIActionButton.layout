PanelWidgetClass UIActionButton {
 size 1 30
 halign center_ref
 valign center_ref
 hexactpos 0
 vexactpos 0
 hexactsize 0
 vexactsize 1
 color 0 0 0 0
 style rover_sim_colorable
 scriptclass "UIActionButton"
 {
  FrameWidgetClass action_wrapper {
   ignorepointer 1
   position 0 0
   size 1 1
   hexactpos 0
   vexactpos 0
   hexactsize 0
   vexactsize 0
   {
    ButtonWidgetClass action_button {
     ignorepointer 0
     size 1 1
     hexactpos 1
     vexactpos 1
     hexactsize 0
     vexactsize 0
     {
      TextWidgetClass action {
       ignorepointer 1
       position 0 0
       size 1 1
       hexactpos 1
       vexactpos 1
       hexactsize 0
       vexactsize 0
       style Light
       text "{BUTTON}"
       font "gui/fonts/sdf_MetronLight24"
       "exact text" 1
       "exact text size" 16
       "text halign" center
       "text valign" center
      }
     }
    }
   }
  }
  PanelWidgetClass action_wrapper_disable {
   visible 0
   clipchildren 0
   ignorepointer 1
   color 0.1569 0.1569 0.1569 0.902
   position 0 0
   size 1 1
   style rover_sim_colorable
  }
 }
}