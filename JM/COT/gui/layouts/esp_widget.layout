PanelWidgetClass JMESPWidget {
	ignorepointer 1
	valign center_ref
	size 300 22
	position 0 0
	hexactpos 1
	vexactpos 1
	hexactsize 1
	vexactsize 1
	color 0 0 0 0
	scriptclass "JMESPWidget"
	{
		PanelWidgetClass CheckboxStyle {
			ignorepointer 1
			color 0.08 0.10 0.13 0.7
			position 0 0
			size 150 1
			hexactsize 1
			style rover_sim_colorable
			{
				ButtonWidgetClass button {
					ignorepointer 0
					position 0 0
					size 1 1
					halign left_ref
					valign center_ref
					hexactpos 1
					vexactpos 1
					hexactsize 0
					vexactsize 0
					{
						TextWidgetClass text_name {
							ignorepointer 1
							position 24 0
							size 1 0.55
							valign center_ref
							halign center_ref
							hexactpos 1
							vexactpos 1
							hexactsize 0
							vexactsize 0
							text ""
							font "gui/fonts/sdf_MetronLight24"
							"exact text" 0
							"text halign" left
							"text valign" center
						}
					}
				}
				PanelWidgetClass checkbox_wrapper
				{
					ignorepointer 0
					color 0 0 0 0
					position 0 0
					size 22 22
					hexactpos 1
					vexactpos 1
					hexactsize 1
					vexactsize 1
					halign left_ref
					valign center_ref
					{
						CheckBoxWidgetClass checkbox {
							ignorepointer 0
							color 1 1 1 1
							position 0 0
							size 20 20
							halign center_ref
							valign center_ref
							hexactpos 0
							vexactpos 0
							hexactsize 1
							vexactsize 1
							scaled 0
							style Default
							checked 0
						}
					}
				}
			}
		}
		PanelWidgetClass JustName {
			ignorepointer 1
			color 0 0 0 0
			position 0 0
			size 1 1
			style rover_sim_colorable
			{
				TextWidgetClass text_name {
					ignorepointer 1
					position 0 0
					size 1 1
					valign center_ref
					halign center_ref
					hexactpos 1
					vexactpos 1
					hexactsize 0
					vexactsize 0
					text ""
					font "gui/fonts/MetronLight16"
					"exact text" 0
					"text halign" left
					"text valign" center
				}
			}
		}
	}
}